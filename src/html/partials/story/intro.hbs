<div class='intro'>
	<h1 class='intro__hed'>{{meta.title}}</h1>
	<p class='intro__dek tk-atlas'>{{meta.description}}</p>
	<p class='intro__byline tk-atlas'>By
		<a href='{{meta.author.url}}' target='_blank'>
			<strong>{{meta.author.name}}</strong>
		</a>
	</p>
</div>
<div class="intro__copy">
	{{#each copy.intro}}
		<p class='prose'>{{{value}}}</p>
	{{/each}}
</div>

<div class="svg-container">

	<div class="misc-chart-elements-container misc-elements">
		<div class='chart-title-div misc-elements'>
			<span class="chart-title-pt1"></span>
			<span class="chart-title-pt2"></span>
			<span class="chart-title-pt3"></span>
			<span class="chart-title-pt4"></span>
			<span class="chart-title-pt5"></span>
			<span class="chart-title-pt6"></span>
		</div>
		<div class='job-selector-div misc-elements'></div>
	</div>
	<svg class="scatter">
	</svg>
</div>
